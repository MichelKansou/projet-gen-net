<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IAuthService" />
                <binding name="BasicHttpBinding_IDispatching" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:58526/AuthService.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IAuthService" contract="AuthServiceReference.IAuthService"
                name="BasicHttpBinding_IAuthService" />
            <endpoint address="http://localhost:58526/Dispatching.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IDispatching" contract="DispatchingServiceReference.IDispatching"
                name="BasicHttpBinding_IDispatching" />
        </client>
    </system.serviceModel>
</configuration>